{
  "name": "linkedin-profile-reader",
  "version": "0.3.0",
  "description": "Parse LinkedIn Profile.pdf exports to JSON Resume format with enhanced parsing accuracy",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "parse-linkedin-pdf": "./bin/parse-linkedin-pdf"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "lint": "eslint . --ext .ts,.js",
    "prepare": "husky install && npm run build",
    "docs": "typedoc"
  },
  "keywords": [
    "linkedin",
    "pdf",
    "parser",
    "resume",
    "json-resume",
    "cv",
    "profile",
    "typescript"
  ],
  "author": "Alexander Pico",
  "license": "MIT",
  "dependencies": {
    "pdfjs-dist": "^5.3.31"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "husky": "^9.1.7",
    "jest": "^30.0.2",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "typedoc": "^0.28.5",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": ["eslint --fix", "prettier --write"]
  }
}
